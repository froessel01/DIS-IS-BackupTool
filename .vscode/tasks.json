{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dotnet: build",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/Programm/BackupTool.csproj"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "dotnet: run",
      "type": "process",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "${workspaceFolder}/Programm/BackupTool.csproj"
      ]
    }
  ]
}
